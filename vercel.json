{
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://cyborg-academy-api.vercel.app/api/v1"
    },
    {
      "source": "/",
      "destination": "https://cyborg-academy-ui.vercel.app/"
    },
    {
      "source": "/:path*",
      "destination": "https://cyborg-academy-ui.vercel.app/:path*"
    }
  ]
}
