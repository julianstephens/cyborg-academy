{
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "includeFiles": "api/index.ts"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/((?!api\/.*).*)",
      "destination": "https://cyborg-academy-ui.vercel.app/"
    },
    {
      "source": "/((?!api\/.*).*)/:path*",
      "destination": "https://cyborg-academy-ui.vercel.app/:path*"
    }
  ]
}
